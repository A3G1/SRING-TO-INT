#include<iostream>
using namespace std;
int main()
{
	int n;
	cin>>n;
	int a[n];
	/*int temp;
	temp=n;*/
	int b[n];
	int k=0;
	for(int i=0;i<n;i++)
	{
		cin>>a[i];
		b[i]=a[i];
	}
	a[n-1]++;
	for(int i=0;i<n;i++)
	{
		cout<<a[i]<<" ";
    }
    for(int i=0;i<n;i++)
    {
    	k = 10 * k + b[i];
    	
	}
	cout<<"\nThe array reprsent the integer "<<k;
}
